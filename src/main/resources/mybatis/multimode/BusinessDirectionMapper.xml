<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.project.multimode.mapper.BusinessDirectionMapper">

    <resultMap id="BusinessDirectionResult" type="com.ruoyi.project.multimode.domain.BusinessDirection">
        <result property="sourceId" column="source_id" />
        <result property="goalId" column="goal_id" />
        <result property="historyId" column="history_id" />
        <result property="taskId" column="task_id" />
        <result property="mode" column="mode" />
        <result property="locationId" column="location_id" />
    </resultMap>

    <select id="findAll" resultMap="BusinessDirectionResult">
        SELECT * FROM business_direction;
    </select>
</mapper>
